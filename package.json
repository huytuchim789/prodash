{
  "name": "@huytuchim789/prodash",
  "version": "1.0.0",
  "type": "module",
  "description": "A modern, tree-shakable JavaScript utility library with TypeScript support",
  "keywords": [
    "utility",
    "lodash",
    "functional",
    "tree-shaking",
    "typescript"
  ],
  "author": "huytuchim789",
  "license": "MIT",
  "homepage": "https://github.com/huytuchim789/prodash#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huytuchim789/prodash.git"
  },
  "bugs": {
    "url": "https://github.com/huytuchim789/prodash/issues"
  },
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js",
      "types": "./types/index.d.ts"
    },
    "./package.json": "./package.json",
    "./*": {
      "import": "./esm/*.js",
      "require": "./cjs/*.js",
      "types": "./types/*.d.ts"
    }
  },
  "scripts": {
    "build": "yarn clean && yarn build:cjs && yarn build:esm && yarn build:umd && yarn build:types",
    "build:cjs": "rollup -c rollup.config.cjs.js",
    "build:esm": "rollup -c rollup.config.esm.js",
    "build:umd": "rollup -c rollup.config.umd.js",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/types",
    "clean": "rimraf dist coverage",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "docs:generate": "typedoc src/index.ts",
    "docs:serve": "http-server docs -p 8080",
    "benchmark": "node benchmarks/index.js",
    "size": "size-limit",
    "prepare": "husky install",
    "prepublishOnly": "yarn test && yarn build",
    "build:visualize": "STATS=true yarn build && open stats.html"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "globals": "^16.3.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jiti": "^2.5.1",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.5.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "size-limit": "^10.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.38.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "size-limit": [
    {
      "path": "esm/index.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "directories": {
    "doc": "docs"
  }
}
